<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:core="clr-namespace:BleClientTester;assembly=BleClientTester"
        xmlns:views="clr-namespace:BleClientTester.Views"
        xmlns:vm="using:BleClientTester.ViewModels"
        xmlns:prism="clr-namespace:Prism.DryIoc;assembly=Prism.DryIoc.Avalonia"
        x:Class="BleClientTester.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        d:DesignHeight="450"
        d:DesignWidth="800"
        Title="{Binding Title}"
        Icon="/Assets/avalonia-logo.ico"
        mc:Ignorable="d">
  <!--<views:MainView />-->
  <ContentControl prism:RegionManager.RegionName="{x:Static core:Constants.ContentRegion}" />
</Window>
